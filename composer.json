{
    "name": "uyoobonga/cloudder",
    "description": "Cloudinary API wrapper for Laravel",
    "keywords": ["laravel", "laravel7",  "laravel8", "cloudinary", "api", "image", "upload"],
    "homepage": "https://github.com/uyoobonga/cloudder",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Uyo-obong A.",
            "email": "uyoobonga@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "cloudinary/cloudinary_php": ">=2.0"
    },
    "require-dev": {
        "laravel/framework": "^7.0|^8.0",
        "phpunit/phpunit": "8.5|^9.0",
        "mockery/mockery": "1.4.*"
    },
    "autoload": {
        "psr-0": {
            "JD\\Cloudder": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "JD\\Cloudder\\CloudderServiceProvider"
            ],
            "aliases": {
                "Cloudder": "JD\\Cloudder\\Facades\\Cloudder"
            }
        }
    }
}
